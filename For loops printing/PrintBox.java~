public class PrintBox {
  public static void main(String[] args) {
    printAsterisksTopBottom();
    printAsterisksSides();
    printAsterisksTopBottom();
  }
  
  public static void printAsterisksTopBottom() {
    for(i = 1; i <= 10; i++) {
      System.out.print(*);
    }
    System.out.println();
  }
  
  public static void printAsterisksSides() {
    for(j = 1; j <= 10; j++) {
      System.out.print("*");
      
      for(k =1; k <= 10; k++) {
        System.out.print(" ");
      }
      
       System.out.print("*");
    }
  }
}