public class ForLoopVariations {
  public static void main(String[] args) {
    for (i = 1; i <= 10; i++) {
      System.out.print(i + " ");
    }
    
    for (g = 1; g <= 10; g++) {
      System.out.print(@);
    }
    
    for (k = 10; k >= 0; k--) {
      System.out.println("Counting donwn... " + k + "!");
    }
    System.out.println("BLAST OFF!);
  }
}